#include <stdio.h>
#include <stdlib.h>

//Este é um programa que calcula a área de um círculo de raio 1
int main()
{
    //Atribuição de variáveis
    float raio,pi,area;
    raio=1;
    pi=3.14;
    area=pi*pow(raio,2);
    //O que será printado
    printf("A area do circulo eh de: %f\n",area);
    return 0;
}
